/*
 * $Log:	TCclear.c,v $
 * Revision 1.1  90/12/12  11:05:30  configtw
 * Initial revision
 * 
*/
/* @@(#)TCclear.c	1.1 10/1/90 */

clear ( addr, numbytes )

    register char *addr;
    register int numbytes;
{
    while ( numbytes-- )
    {
	*addr++ = 0;
    }
}
@


