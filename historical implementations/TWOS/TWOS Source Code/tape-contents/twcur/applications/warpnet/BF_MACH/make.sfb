
SIMDIR	   = ../../../twsim/BF_MACH
#TWDIR	= ../../../tw/BF_MACH
TWDIR	= /usr/bbn/steve/tw






IFLAGS	      =  -DBF_MACH -DFAST_VTIME_MACROS \
		 -I.. -I$(SIMDIR)


CFLAGS	      =   -O  -X130 -DOLDTW
# -X129 -X140

VPATH = ..

LIB_FLAGS     =

 


OBJS = \
		create.o		\
		debug.o	\
		exit.o	\
		obj_tab.o		\
		process.o	\
		router.o	\
		senile.o	\
		update.o		\
		warpinit.o	\
		warpnet.o	


SIMOBJS	      = \
		$(SIMDIR)/simconfig.o\
		$(SIMDIR)/stdine.o\
		$(SIMDIR)/twsd.o\
		$(SIMDIR)/twsp1.o\
		$(SIMDIR)/twsp2.o\
		$(SIMDIR)/long.o\
		$(SIMDIR)/twerror.o\
		$(SIMDIR)/twhelp.o\
		$(SIMDIR)/float.o\
		$(SIMDIR)/stdout.o\
		$(SIMDIR)/itimer.o	\
		$(SIMDIR)/SUNclear.o\
		$(SIMDIR)/copy.o

SIMLIB   = $(SIMDIR)/twsim.a
TWLIB	=  $(TWDIR)/tw.a

all:	warpnet warpnettw


warpnet.sfb:	$(PARALLEL)	$(OBJS)  
		cc $(CFLAGS) $(IFLAGS)  -o warpnet.sfb	\
		$(OBJS)					\
		$(SIMLIB)			\
		-lm -lus

	


warpnettw.sfb:	$(OBJS)  
		cc $(CFLAGS) $(LIB_FLAGS)  $(IFLAGS)  -o warpnettw.sfb	\
		$(OBJS)					\
		$(TWLIB)		\
		-lm

	


.c.o:
	cc $(CFLAGS) $(IFLAGS) -c $*.c
