OBJ_BALT   =    balt.o sortarrayd.o

OBJ_REDUCE =    reduce.o sortarrayd.o

all:		balt reduce

balt:		$(OBJ_BALT)
		cc -o balt  $(OBJ_BALT) 

reduce:		$(OBJ_REDUCE)
		cc -o reduce  $(OBJ_REDUCE)

clean:
		rm -f *.o

.c.o:
	cc -c $*.c


